{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean:pre": "rimraf dist/PrePredios-ofuscado",
    "clean:pro": "rimraf dist/Predios-ofuscado",
    "cpy:pre": "cpy dist/PrePredios-ofuscado/*.js dist/PrePredios/ --flat && npm run clean:pre",
    "cpy:pro": "cpy dist/Predios-ofuscado/*.js dist/Predios/ --flat && npm run clean:pro",
    "ofuscar:pre": "javascript-obfuscator dist/PrePredios --output dist/PrePredios-ofuscado && npm run cpy:pre",
    "build:prequipu": "ng build --base-href=/PrePredios/ --output-path=dist/PrePredios && npm run ofuscar:pre",
    "ofuscar:pro": "javascript-obfuscator dist/Predios --output dist/Predios-ofuscado",
    "build:quipu": "ng build --base-href=/Predios/ --output-path=dist/Predios && npm run ofuscar:pro"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "crypto-js": "^4.1.1",
    "ng-zorro-antd": "^14.0.0",
    "ngx-cookie-service": "^14.0.0",
    "ngx-extended-pdf-viewer": "^18.1.7",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@types/crypto-js": "^4.2.0",
    "@types/jasmine": "~4.0.0",
    "cpy-cli": "^5.0.0",
    "jasmine-core": "~4.1.0",
    "javascript-obfuscator": "^4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "rimraf": "^5.0.5",
    "typescript": "~4.7.2"
  }
}
